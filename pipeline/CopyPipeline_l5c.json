{
	"name": "CopyPipeline_l5c",
	"properties": {
		"activities": [
			{
				"name": "Copy_l5c",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "demo-fs-01/source/items.csv/"
					},
					{
						"name": "Destination",
						"value": "mart.items"
					}
				],
				"typeProperties": {
